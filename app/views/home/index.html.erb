<ul class="home">
	<% @users.each do |user| %>
		<li>
			<%= link_to image_tag(user.profile.photos, { :alt => "Photo of #{user.name}", :width => "175", :height => "125" }), "/#{user.slug}" %>
			<%= link_to user.name, "/#{user.slug}" %>
		</li>
	<% end %>
</ul>





































